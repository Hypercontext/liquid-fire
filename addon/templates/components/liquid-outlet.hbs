<LfGetOutletState as |outletState|>
  {{#liquid-bind
    (lf-lock-model outletState outletName)
    containerId=containerId
    versionEquality=versionEquality
    matchContext=(hash outletName=outletName helperName="liquid-outlet")
    class=class
    use=use
    rules=rules
    containerless=containerless
    growDuration=growDuration
    growPixelsPerSecond=growPixelsPerSecond
    growEasing=growEasing
    shrinkDelay=shrinkDelay
    growDelay=growDelay
    enableGrowth=enableGrowth as |version|
  ~}}
    <WithDynamicVars @outletState={{version}}>
      {{outlet outletName}}
    </WithDynamicVars>
  {{~/liquid-bind~}}
</LfGetOutletState>