{
  "name": "liquid-fire",
  "description": "Animated transitions for Ember applications.",
  "version": "0.31.0",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:fastboot": "qunit fastboot-tests"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-version-checker": "^3.1.3",
    "match-media": "^0.2.0",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "@ember/jquery": "^1.0.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "broccoli-asset-rev": "^3.0.0",
    "command": "0.0.5",
    "commander": "^2.20.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^2.4.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "fastboot": "^2.0.3",
    "git-repo-info": "^2.1.1",
    "github": "git://github.com/mikedeboer/node-github#ca90bf27d5820812c3b76908189d666446ed97cd",
    "jsdom": "^16.7.0",
    "loader.js": "^4.7.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.0.5",
    "qunit-dom": "^1.2.0",
    "rsvp": "^4.8.5",
    "sass": "^1.38.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-animation/liquid-fire",
  "license": "MIT",
  "author": "Edward Faulkner <ef@alum.mit.edu>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "10.24.1",
    "yarn": "1.22.10"
  }
}
